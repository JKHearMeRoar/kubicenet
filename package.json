  "main": "index.js",
  "scripts": {
    "start": "supervisor server.mjs",
    "dev": "npm-run-all --parallel start watch",
    "prod": "node server.mjs",
    "watch": "nodemon -e html,ejs,scss -x npm run css",
    "css-compile": "sass --style compressed --source-map --embed-sources --no-error-css --load-path=node_modules public/scss/:public/css/",
    "css-compile-dev": "sass --source-map --embed-sources --no-error-css --load-path=node_modules public/scss/:public/css/",
    "css-prefix": "postcss --replace public/css/styles.css --use autoprefixer --map",
    "css": "npm-run-all css-compile-dev css-prefix"
  },
